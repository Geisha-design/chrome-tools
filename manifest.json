{
  "manifest_version": 3,
  "name": "Google Label",
  "version": "1.0",
  "description": "A simple Chrome extension for data scraping",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:8999/*",
    "http://127.0.0.1:8999/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
