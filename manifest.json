{
  "manifest_version": 3,
  "name": "数据捕手",
  "version": "2.0",
  "description": "Auto-scrape booking data from Evergreen Shipping",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.evergreen-shipping.cn/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.evergreen-shipping.cn/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
